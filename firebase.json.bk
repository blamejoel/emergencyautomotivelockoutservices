{
  "rewrites": [
    {
      "source": "!/__/**",
      "destination": "/index.html"
    },
    {
      "source": "**/!(*.js|*.html|*.css|*.json|*.svg|*.png|*.jpg|*.jpeg)",
      "destination": "/index.html"
    }
  ],
  "public": "build/unbundled",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ]
}
